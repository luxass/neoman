{
  "name": "@luxass/neoman",
  "type": "module",
  "version": "0.2.1",
  "packageManager": "pnpm@8.6.3",
  "description": "A small yeoman generator alternative",
  "author": {
    "name": "Lucas Norgaard",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "license": "MIT",
  "homepage": "https://github.com/luxass/neoman",
  "repository": {
    "type": "git",
    "url": "https://github.com/luxass/neoman"
  },
  "bugs": {
    "url": "https://github.com/luxass/neoman/issues"
  },
  "keywords": [
    "scaffold",
    "generate"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "vitest",
    "test": "vitest --run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "ejs": "^3.1.9",
    "execa": "^7.1.1"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^3.2.0",
    "@types/ejs": "^3.1.2",
    "@types/node": "^20.2.5",
    "eslint": "^8.41.0",
    "tsup": "^7.0.0",
    "typescript": "^5.0.4",
    "vitest": "^0.31.3"
  }
}
