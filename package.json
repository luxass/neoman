{
  "name": "@luxass/neoman",
  "type": "module",
  "version": "0.2.0",
  "description": "A small yeoman generator alternative",
  "author": {
    "name": "Lucas Norgaard",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "license": "MIT",
  "homepage": "https://github.com/luxass/neoman",
  "repository": {
    "type": "git",
    "url": "https://github.com/luxass/neoman"
  },
  "bugs": {
    "url": "https://github.com/luxass/neoman/issues"
  },
  "keywords": [
    "scaffold",
    "generate"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint --cache {src,tests}/**/*.ts",
    "test": "vitest"
  },
  "dependencies": {
    "ejs": "^3.1.8",
    "execa": "^6.1.0"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^3.0.0",
    "@luxass/prettier-config": "^1.1.9",
    "@types/ejs": "^3.1.1",
    "@types/node": "^18.11.18",
    "eslint": "^8.32.0",
    "prettier": "^2.8.3",
    "tsup": "^6.5.0",
    "typescript": "^5.0.0",
    "vitest": "^0.31.0"
  }
}
